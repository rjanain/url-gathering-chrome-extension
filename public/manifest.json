{
    "name": "Automatic URL Gathering",
    "description": "An  extension that gathers all URLs currently open in browser, allowing them to be easily copied.",
    "version": "0.0.1",
    "manifest_version": 3,
    "offline_enabled": true,
    "background": {
        "service_worker": "vendor/serviceWorker.js"
    },
    "permissions": [
        "tabs",
        "bookmarks",
        "scripting",
        "storage"
    ],
    "icons": {
        "16": "asset/img/16.png",
        "32": "asset/img/32.png",
        "36": "asset/img/36.png",
        "48": "asset/img/48.png",
        "72": "asset/img/72.png",
        "96": "asset/img/96.png",
        "120": "asset/img/120.png",
        "128": "asset/img/128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "asset/img/16.png",
            "32": "asset/img/32.png",
            "36": "asset/img/36.png",
            "48": "asset/img/48.png",
            "72": "asset/img/72.png",
            "96": "asset/img/96.png",
            "120": "asset/img/120.png",
            "128": "asset/img/128.png"
        }
    }
}